<%- include("partials/header") %>
<div class="container mt-5">
	<div class="row">
		<% students.forEach(function(student){%>
		<div class="card col-12 col-md-5 mr-5 mb-5">
			<div class="card-body">
				<div class="card-title">
					<strong>Name:</strong>
					<%= student.Name %>
				</div>
				<div>
					<label><strong>Age:</strong></label>
						<%= student.Age %>
				</div>
				<div>
					<label><strong>Std:</strong></label>
						<%=student.Std %>
				</div>	
				<div>
					<a class="btn btn-sm btn-outline-primary mt-2" href="/students/<%=student._id%>">Read More</a>
				</div>
			</div>	
		</div>

	<% }) %>
		
	</div>
</div>
		
<%- include("partials/footer") %>